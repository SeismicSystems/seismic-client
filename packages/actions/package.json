{
  "name": "seismic",
  "version": "1.0.0",
  "description": "Typescript interface for Seismic",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.ts",
  "types": "dist/index.d.ts",
  "keywords": [
    "Seismic"
  ],
  "license": "MIT",
  "author": "Seismic Systems",
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit; rm tsconfig.tsbuildinfo",
    "build": "bun build ./src/index.ts --outdir ./dist && bun build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --project tsconfig.types.json",
    "postbuild": "rimraf tsconfig.types.tsbuildinfo"
  },
  "dependencies": {
    "viem": "^2.21.50"
  },
  "devDependencies": {
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5"
  }
}
