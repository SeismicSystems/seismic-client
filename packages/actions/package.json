{
  "name": "seismic",
  "version": "1.0.0",
  "description": "Typescript interface for Seismic",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "Seismic"
  ],
  "license": "MIT",
  "author": "Seismic Systems",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit; rm tsconfig.tsbuildinfo",
    "build": "bun build ./src/index.ts --target node --outdir dist --format esm,cjs && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "postbuild": "rimraf tsconfig.types.tsbuildinfo"
  },
  "dependencies": {
    "viem": "^2.21.50"
  },
  "devDependencies": {
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5"
  }
}
