{
  "name": "seismic-viem",
  "version": "1.0.1",
  "description": "Typescript interface for Seismic",
  "type": "module",
  "main": "dist/_cjs/index.js",
  "module": "dist/_esm/index.ts",
  "types": "dist/_types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/_cjs/index.js",
      "import": "./dist/_esm/index.js",
      "types": "./dist/_types/index.d.ts"
    }
  },
  "files": [
    "dist/_cjs/*.js",
    "dist/_esm/*.js",
    "dist/_types/**/*.d.ts",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit; rm tsconfig.tsbuildinfo",
    "build": "bun build:cjs && bun build:esm && bun build:types",
    "build:cjs": "bun build src/index.ts --outdir dist/_cjs --target browser --format cjs --external node:crypto",
    "build:esm": "bun build src/index.ts --outdir dist/_esm --target browser --format esm --external node:crypto",
    "build:types": "tsc --project tsconfig.types.json && tsc-alias --project tsconfig.types.json",
    "postbuild": "rimraf ./dist/tsconfig.types.tsbuildinfo"
  },
  "dependencies": {
    "viem": "^2.21.50"
  },
  "devDependencies": {
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5"
  },
  "license": "MIT",
  "author": "Seismic Systems",
  "homepage": "https://seismic.systems",
  "repository": "https://github.com/SeismicSystems/seismic-client",
  "keywords": [
    "seismic",
    "crypto",
    "shielding",
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3",
    "typescript"
  ]
}